{
  "name": "postman2har",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cli.js",
  "bin": {
    "newman-trace": "node dist/cli.js"
  },
  "scripts": {
    "tsc:build": "tsc",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "esbuild:build": "esbuild --bundle --platform=node --format=cjs --main-fields=module,main --outfile=build/newman-trace.js --tsconfig=./tsconfig.json --sourcemap --minify ./src/cli.ts",
    "esbuild:watch": "esbuild --bundle --platform=node --format=cjs --main-fields=module,main --outfile=build/newman-trace.js --tsconfig=./tsconfig.json --sourcemap --watch ./src/cli.ts",
    "test": "jest --cacheDirectory=.cache/.jest",
    "start": "npm run build && node ./dist/cli.js",
    "clean": "rimraf build dist .cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinswiber/postman2har.git"
  },
  "keywords": [],
  "author": "Kevin Swiber <kswiber@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinswiber/postman2har/issues"
  },
  "homepage": "https://github.com/kevinswiber/postman2har#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.14.23",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "async": "^3.2.3",
    "commander": "^9.0.0",
    "content-type-parser": "^1.0.2",
    "http-string-parser": "^0.0.6",
    "istextorbinary": "^6.0.0",
    "lodash": "^4.17.21",
    "newman": "^5.3.1",
    "require-in-the-middle": "^5.1.0",
    "shimmer": "^1.2.1"
  }
}
